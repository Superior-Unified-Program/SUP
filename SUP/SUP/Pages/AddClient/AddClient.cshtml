@page
@model SUP.Pages.AddClient.AddClientModel
@{
    Layout = null;
}
<!doctype html>
<html>
<head>
    <meta charset="utf-8">
    <script type="text/javascript">
        function submitForm
            {
            var form = document.getElementById("client_info ");
            form.submit();
        }
    </script>
    <title>Add/Edit Client</title>
    <style>
        div {
            width: 100%;
            height：100 %;
            text-align: center;
            padding-top: 5%;
            background-image: white;
            border-collapse: collapse;
        }


        td {
            text-align: center;
        }

        #column_width {
            column-width: auto;
            width: 20%;
            background: #FFD700;
            text-align: left;
        }

        #row_setting {
            background: black;
            height: 80px;
        }

        #title {
            font-size: 30px;
            font-display: block;
        }

        #logo {
            text-align: right;
        }

        #main_info {
            text-align: left;
            float: left;
        }

        #button {
        }
    </style>

</head>

<body>
    <div>
        <table width="100%" height=700px border="0" bordercolor="#000000">
            <tbody>
                <tr>
                    <th scope="col" colspan="2" id="title"><p>Add/Edit Client</p></th>

                </tr>
                <tr>
                    <th scope="row" id="column_width">
                        <a>Welcome</a>

                    </th>
                    <td id="row_setting"><img src="superior_logo.jpg" align=right width=auto height=80px><img src="logo.jpg" align=right width=auto height=80px></td>
                </tr>
                <tr>
                    <th scope="row" id="column_width"> <a href="Search.html">Search</a></th>

                </tr>
                <tr>
                    <th scope="row" id="column_width"> <a href="Add_Client.html">Add Client</a></th>
                    <td rowspan="6" id="main_info" valign="bottom">
                        <b>Required fields(*)</b>
                        <br />
                        <form action="" method="get" id="client_info" onchange="submitForm();">
                            <p>Prefix<input type="text" placeholder="Mr."> First Name* <input type="text" placeholder="Sally" required>  Last Name*<input type="text" placeholder="Sue" required> </p>
                            <p>
                                Company*<select name="company" size="1" required>
                                    <option placeholder="UW Superior " selected="UW superior">UW Superior</option>
                                    <option> </option>
                                </select>

                                </select> Permit <input type="text">
                            </p>
                            <p>Address Line1*<input type="text" placeholder="1710 Weeks Avenue" required> City* <input type="text" placeholder="Superior" required>   </p>
                            <p>
                                Address Line2<input type="text" placeholder="Apt 123">
                                State <select name="state">
                                    <option value="AL">Alabama</option>
                                    <option value="AK">Alaska</option>
                                    <option value="AZ">Arizona</option>
                                    <option value="AR">Arkansas</option>
                                    <option value="CA">California</option>
                                    <option value="CO">Colorado</option>
                                    <option value="CT">Connecticut</option>
                                    <option value="DE">Delaware</option>
                                    <option value="DC">District Of Columbia</option>
                                    <option value="FL">Florida</option>
                                    <option value="GA">Georgia</option>
                                    <option value="HI">Hawaii</option>
                                    <option value="ID">Idaho</option>
                                    <option value="IL">Illinois</option>
                                    <option value="IN">Indiana</option>
                                    <option value="IA">Iowa</option>
                                    <option value="KS">Kansas</option>
                                    <option value="KY">Kentucky</option>
                                    <option value="LA">Louisiana</option>
                                    <option value="ME">Maine</option>
                                    <option value="MD">Maryland</option>
                                    <option value="MA">Massachusetts</option>
                                    <option value="MI">Michigan</option>
                                    <option value="MN">Minnesota</option>
                                    <option value="MS">Mississippi</option>
                                    <option value="MO">Missouri</option>
                                    <option value="MT">Montana</option>
                                    <option value="NE">Nebraska</option>
                                    <option value="NV">Nevada</option>
                                    <option value="NH">New Hampshire</option>
                                    <option value="NJ">New Jersey</option>
                                    <option value="NM">New Mexico</option>
                                    <option value="NY">New York</option>
                                    <option value="NC">North Carolina</option>
                                    <option value="ND">North Dakota</option>
                                    <option value="OH">Ohio</option>
                                    <option value="OK">Oklahoma</option>
                                    <option value="OR">Oregon</option>
                                    <option value="PA">Pennsylvania</option>
                                    <option value="RI">Rhode Island</option>
                                    <option value="SC">South Carolina</option>
                                    <option value="SD">South Dakota</option>
                                    <option value="TN">Tennessee</option>
                                    <option value="TX">Texas</option>
                                    <option value="UT">Utah</option>
                                    <option value="VT">Vermont</option>
                                    <option value="VA">Virginia</option>
                                    <option value="WA">Washington</option>
                                    <option value="WV">West Virginia</option>
                                    <option value="WI">Wisconsin</option>
                                    <option value="WY">Wyoming</option>
                                </select>
                                Zipcode *<input type="text" placeholder="54880" required pattern="[0-9 -]{}">
                            </p>
                            <p>Email address*<input type="email" placeholder="123@emailaddress.net" required> Phone Number<input type="text" placeholder="999-999-999" pattern="[0-9 -]{12}"></p>
                            <p>Actived<input type="checkbox" checked="checked"> Note<input type="text"></p></br>
                            <input type="submit" value="Save" id="button" style="float: right">
                        </form>
                    </td>

                </tr>
                <tr>
                    <th scope="row" id="column_width"> <a href="AboutSUP.html">About SUP</a></th>

                </tr>
                <tr>
                    <th scope="row" rowspan="2" id="column_width" height="50%">&nbsp;</th>

                </tr>
                <tr></tr>
                <tr>
                    <form>
                        <th scope="row" id="column_width"> <a href="Login.html" onclick="if(confirm('Do you want to logout?')==false)return false;">Logout</a></th>
                    </form>
                </tr>
            </tbody>
        </table>
    </div>
</body>
</html>
