@page
@model IndexModel
@{
    //ViewData["Title"] = "Home page";
    Layout = null;
}

<head>
    <style>
        .ribbon{
            width: 100%;
            height：100%;
            text-align: center;
            padding-top: 5%;
            border-collapse: collapse;
        }

        .centeredTd {
            text-align: center;
        }

        #column_width {
            column-width: auto;
            width: 20%;
            background: #FFD700;
            text-align: left;
        }

        #row_setting {
            background: black;
            height: 80px;
        }

        #title {
            font-size: 30px;
            font-display: block;
        }

        #logo {
            text-align: right;
        }
    </style>
</head>

<body>
    <table width="100%" height="100%" border="0" bordercolor="#000000">
		<tbody>
			<tr>
				<th scope="row" id="column_width">
					<a>Welcome</a>

                </th>
                <td class="centeredTd" id="row_setting"><img src="superior_logo.jpg" align=right width=auto; height=80px;><img src="logo.jpg" align=right width=auto height=80px></td>
            </tr>
            <tr>
				<th scope="row" id="column_width"> <a asp-area="" asp-page="Search/Search">Search</a></th>
				<td class="centeredTd" rowspan="6">&nbsp;</td>
			</tr>
			<tr>
				<th scope="row" id="column_width"> <a asp-area="" asp-page="AddClient/AddClient">Add/Edit Client</a></th>

			</tr>
			<tr>
				<th scope="row" id="column_width"> <a asp-area="" asp-page="About/About">About Sup</a></th>

			</tr>
			<tr>
				<th scope="row" id="column_width"> <a asp-area="" asp-page="ribbon">ribbon</a></th>

			</tr>
			<tr>
				<th scope="row" rowspan="2" id="column_width" height="50%">&nbsp;</th>

			</tr>
			<tr>
			</tr>
			<tr>
				<th scope="row" id="column_width"> <a asp-area="" asp-page="Login/Login">Logout</a></th>

			</tr>
		</tbody>
    </table>
</body>